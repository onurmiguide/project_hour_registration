{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs20.x",
      "includeFiles": [
        "templates/**",
        "static/**"
      ]
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
